startup_message off
autodetach on
defscrollback 5000
termcapinfo xterm ti@:te@
termcapinfo xterm-color ti@:te@
hardstatus alwayslastline
hardstatus string '%{gk}[%{G}%H%{g}][%= %{wk}%?%-Lw%?%{=b kR}(%{W}%n*%f %t%?(%u)%?%{=b kR})%{= kw}%?%+Lw%?%?%= %{g}]%{=b C}[%m/%d/%y %c:%s]%{W}'
vbell off
shell -$SHELL
term screen-256color
logtstamp on
logtstamp after 1
# Fix for residual text
 altscreen on

